<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
</head>
<body>
    <div class="container">
        @RenderBody()
    </div>

    @Scripts.Render("~/js/core")
    @Scripts.Render("~/ts/core")
    @RenderSection("scripts", required: false)
</body>
</html>
