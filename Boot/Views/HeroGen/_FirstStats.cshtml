@using Boot.Enums
@using Boot.Helpers
@model List<int>
@for (var i = 0; i < Model.Count; i++)
{
    var a = (StatType)i;
    <div class="col-md-3 col-6">
        <div class="row border">
            <div class="col bg-@a.Color() py-2 px-4">@a.Translation()<span class="float-right font-weight-bold">@Model[i]</span></div>
        </div>
    </div>
}