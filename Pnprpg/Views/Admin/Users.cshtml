@model IQueryable<Pnprpg.DomainService.Models.UserEditModel>
@{
    ViewBag.Title = "Настройка ролей";
}

@using (Html.BeginForm("Users", "Admin", FormMethod.Post, new { @class = "col-md-6" }))
{
    @Html.AntiForgeryToken()

    <table class="table table-striped">
        @Html.EditorForModel()
    </table>
    <input class="btn btn-success" type="submit" value="Сохранить" />
}